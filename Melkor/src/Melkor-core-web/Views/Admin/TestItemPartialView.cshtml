@model Melkor_core_dbhandler.TestContext

<div class="panel panel-body" >
    
    <div class="text-left">
        Test : @Model.Name
    </div>

        <div class="text-left">
            <small> Status : </small>
            @if (Model.Result)
            {
                <span class="fa fa-check"></span>
            }
            else
            {
                <span class="fa fa-exclamation"></span>
            }
        </div>

</div>